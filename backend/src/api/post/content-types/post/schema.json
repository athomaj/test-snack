{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text"
    },
    "address": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "title": {
      "type": "string"
    },
    "seats": {
      "type": "integer",
      "required": false,
      "unique": false
    },
    "datetime": {
      "type": "datetime"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bonus": {
      "type": "string"
    },
    "kitchens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kitchen.kitchen"
    },
    "level": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::level.level"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "diets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::diet.diet"
    },
    "participant": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "events"
    },
    "district": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::district.district"
    },
    "postalCode": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::district.district"
    },
    "moreInfo": {
      "type": "json"
    },
    "infos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::info.info"
    }
  }
}
